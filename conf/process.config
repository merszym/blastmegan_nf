process {
    withName: "FRED_PSEUDOUNIQ" {
        publishDir = [
            path:"pseudouniq",
            mode:"copy",
        ]
        ext.args = "${params.pseudouniq_filterflag} ${params.pseudouniq_minlen} ${params.pseudouniq_mindup}"
    }

    withName: "BLAST2RMA" {
        publishDir = [
            path:"blast",
            mode:"copy",
            pattern:"*.rma6"
        ]
        ext.args = "-ms ${params.rma_ms} -sup ${params.rma_sup}" //-supp ${params.rma_supp}
    }

    withName: "MEGAN_RMA2INFO" {
        publishDir = [
            path:"out/blast",
            mode:"copy",
            pattern:"*.tsv"
        ]
    }
}